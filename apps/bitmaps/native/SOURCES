#endif

/* USB logo */
#ifdef HAVE_LCD_COLOR
#if LCD_WIDTH > 176
usblogo.176x48x16.bmp
#elif LCD_WIDTH >= 128
usblogo.128x37x16.bmp
#endif
#elif LCD_DEPTH > 1 /* greyscale */
