# Define the search sub menu
%menu_start "search" "Search Music by..."
"Artist/Composer" -> albumartist ? artist ~ "" & filename ~ "Music/" -> comment -> title = "fmt_title"
"Album" -> comment ? album ~ "" & filename ~ "Music/" -> title = "fmt_title"
"Title" -> title = "fmt_title" ? title ~ "" & filename ~ "Music/"
"Genre" -> grouping ? grouping ~ "" & filename ~ "Music/" -> genre -> albumartist -> comment -> title = "fmt_title" 
"Sub-Genre" -> genre ? genre ~ "" & filename ~ "Music/" -> albumartist -> comment -> title = "fmt_title" 
"Band Members/Feat. Artists" -> albumartist ? composer ~ "" & filename ~ "Music/"-> comment -> title = "fmt_title"
"Album by year" -> comment ? year = "" & filename ~ "Music/" -> title = "fmt_title"
"Artist between years" -> albumartist ? year >= "" & year <= "" & filename ~ "Music/" -> comment -> title = "fmt_title"
"Album between years" -> comment ? year >= "" & year <= "" & filename ~ "Music/" -> title = "fmt_title"
"Filename" -> filename ? filename ~ ""
"Play Score" -> title = "fmt_score" ? autoscore > "" & filename ~ "Music/"
"User Rating" -> title = "fmt_rating" ? rating > "" & filename ~ "Music/"

# Define the "same as current" sub menu
%menu_start "same" "Same as current..."
"Artist" -> albumartist ? artist = "#artist#" &  filename ~ "Music/"-> comment -> title  = "fmt_title" 
"Album" -> comment ? album = "#album#"  & filename ~ "Music/" -> title = "fmt_title" 
"Title" -> title = "fmt_title" ? filename ~ "#title#"  & filename ~ "Music/"
"Genre" -> grouping ? grouping ~ "#grouping#" &  filename ~ "Music/" -> genre -> albumartist -> comment -> title  = "fmt_title" 

# Define the runtime sub menu
%menu_start "runtime" "Play History"
"Most Played (Plays|Score)" -> title = "fmt_mostplayed" ? playcount > "0" & filename ~ "Music/"
"Recently Played Tracks" -> title = "fmt_lastplayed" ? playcount > "0" & filename ~ "Music/"
"Never Played Tracks" -> albumartist ? playcount == "0"  & filename ~ "Music/"-> comment -> title = "fmt_title"
"Favourite Artists" -> albumartist ? playcount > "3" & autoscore > "85" & filename ~ "Music/"-> comment -> title = "fmt_best_tracks"
"Favourite Albums" -> comment ? playcount > "3" & autoscore > "85" & filename ~ "Music/"-> title = "fmt_best_tracks"
"Favourite Genres" -> grouping ? playcount > "3" & autoscore > "85" & filename ~ "Music/"-> albumartist -> comment -> title = "fmt_best_tracks"
"Favourite Sub-Genres" -> genre ? playcount > "3" & autoscore > "85" & filename ~ "Music/"-> albumartist -> comment -> title = "fmt_best_tracks"
"Recent Favourites" -> title = "fmt_lastplayed" ? playcount > "3" & autoscore > "85"  & filename ~ "Music/"
"New Favourites" -> albumartist ? playcount <= "3" & autoscore > "85" & filename ~ "Music/"-> comment -> title = "fmt_best_tracks"
"Forgotten Favourites" -> title = "fmt_forgotten" ? playcount > "3" & autoscore > "85"  & filename ~ "Music/"

